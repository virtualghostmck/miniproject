@model IEnumerable<Entities.Vendors>

@{
    ViewBag.Title = "Vendor Details";
}

<h2>Vendor Details</h2>



@*<p>
    @Html.ActionLink("Create New", "Create")
</p>*@
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VendorName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VendorAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GSTIN)
        </th>
        <th></th>
    </tr>

@foreach(var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.VendorName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VendorAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GSTIN)
        </td>
        
    </tr>
}



</table>


<div>
    @Html.ActionLink("Back", "Details")
</div>
